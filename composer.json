{
  "name": "ilrweb/legalrepositories",
  "description": "Drupal-based website for ADA and Consent Decree repositories.",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Aaron Froelich",
      "email": "aaronf@cornell.edu",
      "role": "Developer"
    },
    {
      "name": "Jeff Amaral",
      "email": "jeff.amaral@cornell.edu",
      "role": "Developer"
    }
  ],
  "homepage": "https://www.drupal.org/project/drupal",
  "support": {
    "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
    "chat": "https://www.drupal.org/node/314178"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ilrWebServices/union.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ilrWebServices/union_organizer.git"
    }
  ],
  "require": {
    "composer/installers": "^1.9",
    "cornell/union": "dev-union_ng",
    "drupal/core-composer-scaffold": "^9.2",
    "drupal/core-recommended": "^9.2",
    "drupal/field_group": "^3.2",
    "drupal/fixed_text_link_formatter": "^1.6",
    "drupal/paragraphs": "^1.12",
    "drupal/redirect": "^1.6",
    "drupal/samlauth": "^3.5",
    "drupal/title_length": "^1.2",
    "drupal/union_organizer": "dev-union_ng",
    "drush/drush": "^10.6",
    "platformsh/config-reader": "^2.4",
    "symfony/dotenv": "^5.3"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "drupal/core-composer-scaffold": true
    }
  },
  "autoload": {
    "files": ["load.environment.php"]
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "web/profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "web/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    }
  }
}
